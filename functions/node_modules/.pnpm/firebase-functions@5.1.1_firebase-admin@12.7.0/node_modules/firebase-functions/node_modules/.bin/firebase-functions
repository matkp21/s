#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules/firebase-functions/lib/bin/node_modules:/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules/firebase-functions/lib/node_modules:/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules/firebase-functions/node_modules:/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules:/home/user/studio/functions/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules/firebase-functions/lib/bin/node_modules:/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules/firebase-functions/lib/node_modules:/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules/firebase-functions/node_modules:/home/user/studio/functions/node_modules/.pnpm/firebase-functions@5.1.1_firebase-admin@12.7.0/node_modules:/home/user/studio/functions/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin/firebase-functions.js" "$@"
else
  exec node  "$basedir/../../lib/bin/firebase-functions.js" "$@"
fi
